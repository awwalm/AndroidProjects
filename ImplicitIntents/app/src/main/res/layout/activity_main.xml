<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/implicitIntenetsLinearLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_centerInParent="false"
    android:layout_centerHorizontal="false"
    android:layout_centerVertical="false"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="100dp"
        android:minHeight="70dp"
        android:orientation="horizontal">

        <Button
            android:id="@+id/btnwebaddress"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="16dp"
            android:layout_marginRight="16dp"
            android:background="#F1EEEE"
            android:fontFamily="@font/heebo_thin"
            android:gravity="center"
            android:lines="2"
            android:minHeight="50dip"
            android:shadowColor="#009688"
            android:text="@string/webaddress"
            android:textAllCaps="false"
            android:textSize="20sp"></Button>


    </LinearLayout>

    <LinearLayout
        android:id="@+id/mapLinearLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="25dp"
        android:minHeight="70dp"
        android:orientation="horizontal">

        <Button
            android:id="@+id/btnmap"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="16dp"
            android:layout_marginRight="16dp"
            android:background="#F1EEEE"
            android:fontFamily="@font/heebo_thin"
            android:gravity="center"
            android:lines="2"
            android:minHeight="50dip"
            android:shadowColor="#009688"
            android:text="@string/newmap"
            android:textAllCaps="false"
            android:textSize="20sp"></Button>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/phoneLinearLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="25dp"
        android:minHeight="70dp"
        android:orientation="horizontal">

        <Button
            android:id="@+id/btnphonenumber"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="16dp"
            android:layout_marginRight="16dp"
            android:background="#F1EEEE"
            android:fontFamily="@font/heebo_thin"
            android:gravity="center"
            android:lines="2"
            android:minHeight="50dip"
            android:shadowColor="#009688"
            android:text="@string/newphone"
            android:textAllCaps="false"
            android:textSize="20sp"></Button>

    </LinearLayout>

</LinearLayout>